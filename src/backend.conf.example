[main]
# main domain
domain=example.com

# default ttl
ttl=432000

# default IP address for non-wildcard entries
ipaddress=127.0.0.1


# SOA
[soa]
# serial number
id=1
# Hostmaster email address
hostmaster=hostmaster@example.com
# Name server
ns=ns1.example.com


# nameservers
[nameservers]
ns1.example.com=127.0.0.1
ns2.example.com=127.0.0.1

[additional_cnames]
www=business.example.com

# txt only one record can be entered for each key.
[additional_txt]
_acme-challenge=102245cd71a62b1581a82c976670bc6d49695d38a60524b3615c4581e64d4f9c
